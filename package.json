{
  "name": "for-hrs-project-with-webpack",
  "version": "1.0.0",
  "main": "app.js",
  "author": "masayuki suzuki",
  "license": "MIT",
  "scripts": {
    "imagemin": "imagemin assets/img/*.* --out-dir=./www/dist/img/ --plugin=pngquant",
    "watch-imagemin": "onchange 'assets/img/**/*' -- npm run imagemin",
    "dev": "webpack --env.prod --mode development --config ./config/webpack.config.js --watch --progress",
    "server": "browser-sync start --config ./config/bs-config.js",
    "start": "npm-run-all -p dev watch-imagemin server",
    "build": "webpack --env.prod --mode development --progress --config ./config/webpack.config.js",
    "prod": "webpack --mode production --config ./config/webpack.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-stage-0": "^7.0.0-beta.52",
    "@babel/preset-stage-1": "^7.0.0-beta.52",
    "@babel/runtime": "^7.0.0-beta.52",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.0.0-beta",
    "browser-sync": "^2.24.5",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.0-alpha",
    "imagemin": "^5.3.1",
    "imagemin-cli": "^3.0.0",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.1",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.0.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.7",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.6.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.52",
    "smooth-scroll": "^14.2.0",
    "swiper": "^4.3.3",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tsutils": "^2.27.2"
  }
}
